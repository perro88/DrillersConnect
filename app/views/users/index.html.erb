<%# User Index Page %>
<h1>USERS</h1>
<table>
  <tr>
    <th>Name</th>
    <th>Available</th>
  </tr>
  <% @users.each do |user|%>
  <% if !user.has_role? :employer%>
    <tr>
      <td><%= link_to user.full_name, user_path(user) %></td>
      <% if user.information.last %>
      <td><%= user.information.last.available %></td>
      <% end %>
    </tr>
    <% end %>
  <% end %>
</table>
