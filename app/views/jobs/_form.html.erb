<%= form_with model: @new_job, local: true do |f| %>
<p>
<%= f.text_field :position, placeholder: "Position" %>
</p>
<p>
<%= f.text_field :place, placeholder: "Location" %>
</p>
<p>
<%= f.text_field :contract, placeholder: "Contract Length" %>
</p>
<p>
  <%= f.text_area :description, placeholder: "Description"%>
</p>
<p>
  <%= f.submit %>
</p>
<% end %>