<h1>JOBS</h1>
<% if user_signed_in? %>
<p><%= link_to "Add a New Job", new_job_path%> </p>
<% end %>
<h2>Current Listings</h2>
<table>
    <% @users.each do |user| %>
    <% user.jobs.each do |list|%>
  <tr>
    <td> <%= link_to list.position, job_path(list)%></td>
    <td> <%= list.place %> </td>
  </tr>
  <% end %>
  <% end %>
</table>